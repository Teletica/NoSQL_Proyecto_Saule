@model IEnumerable<NoSQL_Proyecto_Saule.Models.Rol>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreRol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descripcionRol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estadoRol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreRol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descripcionRol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estadoRol)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |

            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
