@model IEnumerable<NoSQL_Proyecto_Saule.Models.Compras>

@{
    ViewBag.Title = "List";
}

<h2>Lista de Compras</h2>

<p>
    @Html.ActionLink("Registrar Nueva Compra", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fechaCompra)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.productosCompra)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fechaCompra)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.productosCompra)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
